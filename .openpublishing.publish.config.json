{
  "need_pr_comments": false,
  "docsets_to_publish": [
    {
      "docset_name": "core-docs",
      "build_source_folder": ".",
      "build_output_subfolder": ".",
      "locale": "ru-ru",
      "monikers": [
        "netframework-4.5.1",
        "netframework-4.5.2",
        "netframework-4.5",
        "netframework-4.6",
        "netframework-4.6.1",
        "netframework-4.6.2",
        "netframework-4.7",
        "netcore-1.0",
        "netcore-1.1",
        "netcore-2.0",
        "netstandard-1.0",
        "netstandard-1.1",
        "netstandard-1.2",
        "netstandard-1.3",
        "netstandard-1.4",
        "netstandard-1.5",
        "netstandard-1.6",
        "netstandard-2.0",
        "xamarinios-10.8",
        "xamarinandroid-7.1",
        "xamarinmac-3.0"
      ],
      "open_to_public_contributors": true,
      "type_mapping": {
        "Conceptual": "Content",
        "ManagedReference": "Content",
        "RestApi": "Content"
      },
      "build_entry_point": "docs",
      "customized_template_paths": [
        "_dependentPackages/memberpage.plugins/content"
      ]
    }
  ],
  "notification_subscribers": [
    "CnE_Content_Brno@moravia.com",
    "CnE_UE_Bugfixing_ENG@moravia.com"
  ],
  "branches_to_filter": [
    "live"
  ],
  "git_repository_url_open_to_public_contributors": "https://github.com/dotnet/docs.ru-ru",
  "git_repository_branch_open_to_public_contributors": "live",
  "continue_with_document_error": true,
  "skip_source_output_uploading": false,
  "need_preview_pull_request": true,
  "contribution_branch_mappings": {},
  "dependent_repositories": [
    {
      "path_to_root": "_themes",
      "url": "https://github.com/Microsoft/templates.docs.msft.ru-ru",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "_themes.pdf",
      "url": "https://github.com/Microsoft/templates.docs.msft.pdf",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "_repo.en-us",
      "url": "https://github.com/dotnet/docs",
      "branch": "live",
      "branch_mapping": {}
    },
    {
      "path_to_root": "dotnet-xref",
      "url": "https://github.com/MicrosoftDocs/dotnet-xref",
      "branch": "live",
      "branch_mapping": {
        "master": "master",
        "live": "live"
      }
    }
  ],
  "branch_target_mapping": {
    "master": [
      "Publish"
    ]
  },
  "need_generate_pdf_url_template": false,
  "need_generate_pdf": false,
  "need_generate_intellisense": false,
  "dependent_packages": [
    {
      "id": "memberpage.plugins",
      "nuget_feed": "https://www.myget.org/F/docfx/api/v2",
      "path_to_root": "_dependentPackages/memberpage.plugins",
      "target_framework": "net45",
      "version": "latest"
    }
  ],
  "Targets": {
    "Pdf": {
      "template_folder": "_themes.pdf"
    }
  }
}
